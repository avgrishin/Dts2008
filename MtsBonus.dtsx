<![CDATA[/*
   Microsoft SQL Server Integration Services Script Task
   Write scripts using Microsoft Visual C# 2008.
   The ScriptMain is the entry point class of the script.
*/

using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Net;
using System.Text.RegularExpressions;
using System.IO;
using System.Text;
using Microsoft.Win32;

namespace ST_03f34e36524b4f308f2110e570f93170.csproj
{
  [System.AddIn.AddIn("ScriptMain", Version = "1.0", Publisher = "", Description = "")]
  public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
  {

    #region VSTA generated code
    enum ScriptResults
    {
      Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
      Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    };
    #endregion

    /*
		The execution engine calls this method when the task executes.
		To access the object model, use the Dts property. Connections, variables, events,
		and logging features are available as members of the Dts property as shown in the following examples.

		To reference a variable, call Dts.Variables["MyCaseSensitiveVariableName"].Value;
		To post a log entry, call Dts.Log("This is my log text", 999, null);
		To fire an event, call Dts.Events.FireInformation(99, "test", "hit the help message", "", 0, true);

		To use the connections collection use something like the following:
		ConnectionManager cm = Dts.Connections.Add("OLEDB");
		cm.ConnectionString = "Data Source=localhost;Initial Catalog=AdventureWorks;Provider=SQLNCLI10;Integrated Security=SSPI;Auto Translate=False;";

		Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
		
		To open Help, press F1.
	*/

    public void Main()
    {
      //Registry.SetValue(@"HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Internet Settings", "AutoConfigURL", "http://pp.srv.nikoil.ru/jslib/proxy-bc.pac");
      //object r1 = Registry.GetValue(@"HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Internet Settings", "AutoConfigURL", "");
      HttpWebRequest wr;
      StreamWriter requestWriter;
      string postData;

      wr = (HttpWebRequest)WebRequest.Create("http://www.bonus.mts.ru/ru/pmsdata.html?target=mts_bonus/index/index_html");
      wr.Proxy = new WebProxy("proxy-cluster-bc.srv.nikoil.ru", 8080);
      wr.Proxy.Credentials = CredentialCache.DefaultCredentials;
      wr.Method = WebRequestMethods.Http.Post;
      wr.Timeout = 600000;
      wr.ReadWriteTimeout = 600000;
      wr.Headers.Add("Accept-Language", "ru");
      wr.UserAgent = "Mozilla/4.0 (compatible; MSIE 8.0; Windows NT 5.1; Trident/4.0; .NET CLR 1.1.4322; .NET CLR 2.0.50727; .NET CLR 3.0.04506.648; .NET CLR 3.5.21022; .NET CLR 3.0.4506.2152; .NET CLR 3.5.30729)";
      wr.ContentType = "application/x-www-form-urlencoded";
      postData = "region=12";
      wr.ContentLength = postData.Length;
      CookieContainer cookie = new CookieContainer();
      wr.CookieContainer = cookie;
      requestWriter = new StreamWriter(wr.GetRequestStream());
      requestWriter.Write(postData);
      requestWriter.Close();
      wr.GetResponse().Close();
      
      wr = (HttpWebRequest)WebRequest.Create("http://www.bonus.mts.ru/ru/pmsdata.html?target=mts_bonus/index/catalogue_html");
      wr.Proxy = new WebProxy("proxy-cluster-bc.srv.nikoil.ru", 8080);
      wr.Proxy.Credentials = CredentialCache.DefaultCredentials;
      wr.Timeout = 600000;
      wr.ReadWriteTimeout = 600000;
      wr.Accept = "text/html, application/xhtml+xml, */*";
      wr.Headers.Add("Accept-Language", "ru-RU");
      wr.UserAgent = "User-Agent: Mozilla/5.0 (compatible; MSIE 9.0; Windows NT 6.1; WOW64; Trident/5.0)";
      wr.Headers.Add("Pragma", "no-cache");
      wr.CookieContainer = cookie;

      using (HttpWebResponse hwr = (HttpWebResponse)wr.GetResponse())
      {
        using (Stream receiveStream = hwr.GetResponseStream())
        {
          using (StreamReader sr = new StreamReader(receiveStream))
          {
            string s = sr.ReadToEnd();
            Match r = Regex.Match(s, "var discountInfo = jQuery.parseJSON.'{\"discountOn\":true,\"prize\":{\"prizeId\":(\\d+),\"beginDate\":\"(.+?)\",\"endDate\":\"(.+?)\",\"moscowTime\":\".+?\",\"prizeName\":\"(.+?)\",\"prizeType\":.*?}}'.;", RegexOptions.Singleline);
            if (r.Success)
            {
              Dts.Variables["db"].Value = r.Groups[2].Value;
              Dts.Variables["de"].Value = r.Groups[3].Value;
              Dts.Variables["prizeName"].Value = r.Groups[4].Value;
              Dts.Variables["discount"].Value = true;
            }
          }
        }
      }
      if ((bool)Dts.Variables["discount"].Value == true)
      {
        string str = "";
        if (File.Exists("c:\\tmp\\mts.txt"))
        {
          using (StreamReader sr = new StreamReader("c:\\tmp\\mts.txt", Encoding.GetEncoding(1251)))
          {
            str = sr.ReadLine();
          }
        }
        if (str != string.Format("{0:dd.MM.yyyy} {1:dd.MM.yyyy} {2} {3}", Dts.Variables["db"].Value, Dts.Variables["de"].Value, Dts.Variables["prizeName"].Value, Dts.Variables["discount"].Value))
        {
          using (StreamWriter sw = new StreamWriter("c:\\tmp\\mts.txt", false, Encoding.GetEncoding(1251)))
          {
            sw.WriteLine("{0:dd.MM.yyyy} {1:dd.MM.yyyy} {2} {3}", Dts.Variables["db"].Value, Dts.Variables["de"].Value, Dts.Variables["prizeName"].Value, Dts.Variables["discount"].Value);
          }
          using (StreamWriter sw = new StreamWriter("c:\\tmp\\mts.bonus", true, Encoding.GetEncoding(1251)))
          {
            sw.WriteLine("{0} {1:dd.MM.yyyy} {2:dd.MM.yyyy} {3} {4}", DateTime.Now, Dts.Variables["db"].Value, Dts.Variables["de"].Value, Dts.Variables["prizeName"].Value, Dts.Variables["discount"].Value);
          }
          Dts.Variables["sendmail"].Value = true;
        }
      }
      Dts.TaskResult = (int)ScriptResults.Success;
    }
  }
}]]></ProjectItem><ProjectItem Name="\properties\resources.designer.cs">
