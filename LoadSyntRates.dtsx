<property id="5" name="SqlCommand" dataType="System.String" state="default" isArray="false" description="The SQL command to be executed." typeConverter="" UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91" containsID="false" expressionType="None">declare @d smalldatetime = (select top 1 WorkDate from tWorkDate where WorkDate &lt; getdate()-1 order by WorkDate desc)
--set @d = '200715'

select
  dc.DiasoftBOID,
  CourseCurrent = t.Price,
  CourseClose = ep.[Close],
  CourseAvg = ep.[Avg],
  CourseMax = ep.High,
  CourseMin = ep.Low,
  ep.[Open],
  CourseBid = ebo.Bid,
  CourseOffer = ebo.Offer,
  CourseVal = dc1.DiasoftBOID,
  ActualizationDateTime = @d,
  Nkd = case when s.SecType = 2 then dbo.uf_avgGetCoupon(t.SecurityID, @d, t.FundID) end,
 InstitutionID = 10000096494
from (
  select
    a,
    SecurityID,
    Price,
    FundID,
    ExchangeSecurityID,
    row_number() over (partition by SecurityID order by a) r
  from (
    select
      a = 1,
      SecurityID,
      Price = [Avg],
      FundID,
      ExchangeSecurityID
    from (
      select
        SecurityID = coalesce(s.ParentID, s.SecurityID),
        ep.RateDate,
        es.FundID,
        ep.[Avg],
        row_number() over (partition by coalesce(s.ParentID, s.SecurityID) order by ep.RateDate desc) r,
        es.ExchangeSecurityID
      from tExchangeSecurity es 
       join tSecurity s on s.SecurityID = es.SecurityID and s.SecType in (0,2,15) and (s.SecType &lt;&gt; 2 or coalesce(nullif(s.DateEndFact, '19000101'), s.DateEnd) &gt; @d)
       join tExchangePrice ep on ep.ExchangeSecurityID = es.ExchangeSecurityID and ep.RateDate between @d-89 and @d and ep.Avg &gt; 0
       left join tExchangeRate er on er.ExchangeSecurityID = es.ExchangeSecurityID and er.RateDate = ep.RateDate
       left join tExchangeBidOffer ebd on ebd.ExchangeSecurityID = es.ExchangeSecurityID and ebd.RateDate = ep.RateDate
      where es.RawDataProviderID = 11
    ) t
    where r = 1

    union all

    select
      a = 2,
      SecurityID = coalesce(s.ParentID, s.SecurityID),
      Price = er.ClosePrice,
      es.FundID,
      es.ExchangeSecurityID
    from tExchangeSecurity es 
     join tSecurity s on s.SecurityID = es.SecurityID and s.SecType in (0,2,15) and (s.SecType &lt;&gt; 2 or coalesce(nullif(s.DateEndFact, '19000101'), s.DateEnd) &gt; @d)
     join tExchangeRate er on er.ExchangeSecurityID = es.ExchangeSecurityID and er.RateDate = @d and er.ClosePrice &gt; 0
    where es.RawDataProviderID = 11

    union all

    select
      a = 3,
      SecurityID,
      Price = [Close],
      FundID,
      ExchangeSecurityID
    from (
      select SecurityID = coalesce(s.ParentID, s.SecurityID), ep.[Close], row_number() over (partition by coalesce(s.ParentID, s.SecurityID) order by ep.RateDate desc) r, es.FundID, es.ExchangeSecurityID
      from tExchangeSecurity es 
       join tSecurity s on s.SecurityID = es.SecurityID and s.SecType in (0,2,15) and (s.SecType &lt;&gt; 2 or coalesce(nullif(s.DateEndFact, '19000101'), s.DateEnd) &gt; @d)
       join tExchangePrice ep on ep.ExchangeSecurityID = es.ExchangeSecurityID and ep.RateDate between @d-179 and @d and ep.[Close] &gt; 0
      where es.RawDataProviderID = 11
    ) t
    where r = 1

    union all

    select
      a = 4,
      SecurityID,
      Price = [Close],
      FundID,
      ExchangeSecurityID
    from (
      select SecurityID = coalesce(s.ParentID, s.SecurityID), ep.[Close], row_number() over (partition by coalesce(s.ParentID, s.SecurityID) order by ep.RateDate desc) r, fundID = coalesce(ebd.FundID, 39192),es.ExchangeSecurityID
      from tExchangeSecurity es 
       join tSecurity s on s.SecurityID = es.SecurityID and s.SecType in (0,2,15) and (s.SecType &lt;&gt; 2 or coalesce(nullif(s.DateEndFact, '19000101'), s.DateEnd) &gt; @d)
       join tExchangePrice ep on ep.ExchangeSecurityID = es.ExchangeSecurityID and ep.RateDate between @d-29 and @d and ep.[Close] &gt; 0
       left join tExchangeBidOffer ebd on ebd.ExchangeSecurityID = es.ExchangeSecurityID and ebd.RateDate = ep.RateDate
      where es.RawDataProviderID = 17
    ) t
    where r = 1

    union all

    select
      a = 5,
      SecurityID = coalesce(s.ParentID, s.SecurityID), 
      Price = case when ebd.Offer = 0 then ebd.Bid when (ebd.Offer-ebd.Bid)/ebd.Bid &lt; .1 then (ebd.Offer+ebd.Bid)/2 else ebd.Bid end,
      ebd.FundID,
      es.ExchangeSecurityID
    from tExchangeSecurity es 
     join tSecurity s on s.SecurityID = es.SecurityID and s.SecType in (0,2,15) and (s.SecType &lt;&gt; 2 or coalesce(nullif(s.DateEndFact, '19000101'), s.DateEnd) &gt; @d)
     join tExchangeBidOffer ebd on ebd.ExchangeSecurityID = es.ExchangeSecurityID and ebd.RateDate = @d and ebd.Bid &gt; 0
    where es.RawDataProviderID = 17

    union all

    select
      a = 6,
      SecurityID = coalesce(s.ParentID, s.SecurityID), 
      Price = ep.[Close],
      es.FundID,
      es.ExchangeSecurityID
    from tExchangeSecurity es 
     join tSecurity s on s.SecurityID = es.SecurityID and s.SecType = 2
     join tExchangePrice ep on ep.ExchangeSecurityID = es.ExchangeSecurityID and ep.RateDate = @d and ep.[Close] &gt; 0
    where es.RawDataProviderID = 25
  ) t
) t
 join tSecurity s on s.SecurityID = t.SecurityID
 left join tExchangePrice ep on ep.ExchangeSecurityID = t.ExchangeSecurityID and ep.RateDate = @d
 left join tExchangeBidOffer ebo on ebo.ExchangeSecurityID = t.ExchangeSecurityID and ebo.RateDate = @d
 join tDictionariesConnection dc on dc.CompositeID = t.SecurityID and dc.Dictionary = 1104993180
 join tDictionariesConnection dc1 on dc1.CompositeID = t.FundID and dc1.Dictionary = 1104993180
where r = 1</property>
