on read uncommitted &#xA;&#xA;    select @RateID = 0&#xA;    &#xA;    select @RateID = RateID&#xA;    from tRate (INDEX XAK1tRate)&#xA;    where RateType = 0&#xA;    and ObjectID = @ObjectID&#xA;    and InstitutionID = @InstitutionID&#xA;    and Date_Time = @DateTime&#xA;    at isolation read uncommitted &#xA;    &#xA;    if isnull(@RateID, 0) = 0&#xA;    begin&#xA;        exec Rate_Insert&#xA;         @RateID        = @RateID out,&#xA;         @RateType      = 0,&#xA;         @ObjectID      = @ObjectID,&#xA;         @InstitutionID = @InstitutionID,&#xA;         @DateTime      = @DateTime,&#xA;         @FundID        = @FundID,&#xA;         @Bid           = @Bid,&#xA;         @Offer         = @Offer,&#xA;         @Course        = @Course,&#xA;         @FirstCourse   = @FirstCourse,&#xA;         @LastCourse    = @LastCourse,&#xA;         @Value         = @Value,&#xA;         @ValuePrice    = @ValuePrice,&#xA;         @MinCourse     = @MinCourse,&#xA;         @MaxCourse     = @MaxCourse,&#xA;         @BestBid       = @BestBid,&#xA;         @BestOffer     = @BestOffer,&#xA;         @Receipt       = @Receipt,&#xA;         @CountDeal     = @CountDeal,&#xA;         @Interest      = @Interest,&#xA;         @AverageCour   = @AverageCour&#xA;    end&#xA;    else&#xA;    begin&#xA;        exec Rate_Update&#xA;         @RateID        = @RateID,&#xA;         @DateTime      = @DateTime,&#xA;         @FundID        = @FundID,&#xA;         @Bid           = @Bid,&#xA;         @Offer         = @Offer,&#xA;         @Course        = @Course,&#xA;         @FirstCourse   = @FirstCourse,&#xA;         @LastCourse    = @LastCourse,&#xA;         @Value         = @Value,&#xA;         @ValuePrice    = @ValuePrice,&#xA;         @MinCourse     = @MinCourse,&#xA;         @MaxCourse     = @MaxCourse,&#xA;         @BestBid       = @BestBid,&#xA;         @BestOffer     = @BestOffer,&#xA;         @Receipt       = @Receipt,&#xA;         @CountDeal     = @CountDeal,&#xA;         @Interest      = @Interest,&#xA;         @AverageCour   = @AverageCour&#xA;    end&#xA;&#xA;    select @ObjectID = min(DiasoftBOID)&#xA;    from tempdb..pSyntRates1&#xA;    where DiasoftBOID &gt; @ObjectID&#xA;    at isolation read uncommitted &#xA;end&#xA;" SQLTask:CodePage="1251" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
